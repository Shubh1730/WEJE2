
package com.jspiders.SingletonAccount.Transaction;

import com.jspiders.SingletonAccount.Account.Account;

public class CheckBalance {
	private Account account;
	
	public CheckBalance(Account account) {
		super();
		this.account = account;
	}

	public void checkBalance()
	{
		System.out.println("Your Balance is "+ account.accountBalance);
	}
}
