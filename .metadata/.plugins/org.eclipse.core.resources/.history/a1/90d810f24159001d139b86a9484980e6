package com.jspiders.MobilePattern.Builder;

public class MobileBuilder {
	private String brand;
	private String model;
	private String color;
	private double price;
	private int ram;
	private int memory;
	private String os;
	private int f_cam;
	private int r_cam;
	private double disp_size;
	
	public MobileBuilder brand (String brand) {
		this.brand=brand;
		return this;
	}
	
	public MobileBuilder model (String model) {
		this.model=model;
		return this;
	}
	
	public MobileBuilder color (String color) {
		this.color=color;
		return this;
	}
	
	public MobileBuilder os (String os) {
		this.os=os;
		return this;
	}
}
